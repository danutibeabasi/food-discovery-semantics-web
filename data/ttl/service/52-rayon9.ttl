@prefix ns1: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://rayon9.coopcycle.org> a ns1:Service ;
    ns1:areaServed [ a ns1:Place ;
            ns1:address [ a ns1:PostalAddress ;
                    ns1:addressCountry "be" ;
                    ns1:addressLocality "Liège" ] ;
            ns1:geo [ a ns1:GeoCoordinates ;
                    ns1:latitude 5.062473e+01 ;
                    ns1:longitude 5.52905e+00 ] ] ;
    ns1:description "<p>RAYON9 a été fondée en 2016 à Liège (Belgique) par un petit groupe de citoyennes et de citoyens concerné.e.s par les problèmes de mobilité et d’emploi dans leur ville. Constituée sous la forme d’une société coopérative à finalité sociale, elle est aujourd’hui propulsée par plus de 100 coopérateurs et coopératrices. Leur objectif : rendre la ville plus verte et promouvoir l’emploi des jeunes !</p><p>RAYON9 transporte essentiellement des produits alimentaires (fruits, légumes, pain, ...) pour des magasins ou des restaurants mais aussi du courrier, de la presse, de l'électroménager ou même des sapins ! Les client.e.s principaux.ales sont celles et ceux qui nous confient le dernier kilomètre pour des livraisons en entreprise (corbeilles de fruits) ou à domicile (produits de la ferme).</p>"@fr ;
    ns1:name "RAYON9" ;
    ns1:provider [ a ns1:Organization ;
            ns1:name "CoopCycle" ] ;
    ns1:sameAs <https://www.facebook.com/rayon9coop> ;
    ns1:serviceType "DeliveryService" ;
    ns1:url <https://rayon9.coopcycle.org> .

